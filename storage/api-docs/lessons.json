    "resourcePath": "/lessons",
    "apis": [
        {
            "path": "/lessons/categories",
            "operations": [
                {
                    "method": "GET",
                    "summary": "get all lessons categories",
                    "nickname": "Album",
                    "type": "Album",
                    "parameters": [
                        {
                            "paramType": "query",
                            "name": "lang",
                            "type": "string",
                            "required": false,
                            "allowMultiple": false,
                            "description": "iso 2 code of language needed ex ar,en,fa if failed will return data with default language"
                        }
                    ],
                    "responseMessages": [
                        {
                            "code": 400,
                            "message": "Invalid ID supplied"
                        },
                        {
                            "code": 404,
                            "message": "Not found"
                        }
                    ],
                    "notes": "Returns all lessons categories",
                    "produces": [
                        "application/json"
                    ],
                    "authorizations": {}
                }
            ]
        },
