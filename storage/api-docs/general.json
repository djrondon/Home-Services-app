{
    "basePath": "http://api.sadaislam.com/v1",
    "swaggerVersion": "2.0",
    "apiVersion": "1",
    "resourcePath": "/general",
    "apis": [
        {
            "path": "/general/latest",
            "operations": [
                {
                    "method": "GET",
                    "summary": "get all latest",
                    "nickname": "Latest Query",
                    "type": "Series",
                    "parameters": [
                        {
                            "paramType": "query",
                            "name": "lang",
                            "type": "string",
                            "required": false,
                            "allowMultiple": false,
                            "description": "iso 2 code of language needed ex ar,en,fa if failed will return data with default language"
                        }
                    ],
                    "responseMessages": [
                        {
                            "code": 400,
                            "message": "Invalid ID supplied"
                        },
                        {
                            "code": 404,
                            "message": "Not found"
                        }
                    ],
                    "notes": "Returns all latest",
                    "produces": [
                        "application/json"
                    ],
                    "authorizations": {}
                }
            ]
        },
        {
            "path": "/general/most",
            "operations": [
                {
                    "method": "GET",
                    "summary": "get all most",
                    "nickname": "Most Query",
                    "type": "Series",
                    "parameters": [
                        {
                            "paramType": "query",
                            "name": "lang",
                            "type": "string",
                            "required": false,
                            "allowMultiple": false,
                            "description": "iso 2 code of language needed ex ar,en,fa if failed will return data with default language"
                        }
                    ],
                    "responseMessages": [
                        {
                            "code": 400,
                            "message": "Invalid ID supplied"
                        },
                        {
                            "code": 404,
                            "message": "Not found"
                        }
                    ],
                    "notes": "Returns all most",
                    "produces": [
                        "application/json"
                    ],
                    "authorizations": {}
                }
            ]
        }
    ]
}