{
    "basePath": "http://api.sadaislam.com/v1",
    "swaggerVersion": "2.0",
    "apiVersion": "1",
    "resourcePath": "/auth",
    "apis": [
        {
            "path": "/auth/login",
            "operations": [
                {
                    "method": "POST",
                    "summary": "login",
                    "nickname": "login",
                    "parameters": [
                        {
                            "paramType": "form",
                            "name": "username",
                            "type": "string",
                            "required": true,
                            "allowMultiple": false,
                            "description": "user name"
                        },
                        {
                            "paramType": "form",
                            "name": "password",
                            "type": "string",
                            "required": true,
                            "allowMultiple": false,
                            "description": "password"
                        }
                    ],
                    "responseMessages": [
                        {
                            "code": 400,
                            "message": "Invalid ID supplied"
                        },
                        {
                            "code": 404,
                            "message": "Not found"
                        }
                    ],
                    "notes": "",
                    "produces": [
                        "application/json"
                    ],
                    "authorizations": {}
                }
            ]
        },
        {
            "path": "/auth/register",
            "operations": [
                {
                    "method": "POST",
                    "summary": "register",
                    "nickname": "register",
                    "parameters": [
                        {
                            "paramType": "form",
                            "name": "name",
                            "type": "string",
                            "required": true,
                            "allowMultiple": false,
                            "description": "user full name"
                        },
                        {
                            "paramType": "form",
                            "name": "email",
                            "type": "email",
                            "required": true,
                            "allowMultiple": false,
                            "description": "user email"
                        },
                        {
                            "paramType": "form",
                            "name": "username",
                            "type": "string",
                            "required": true,
                            "allowMultiple": false,
                            "description": "username"
                        },
                        {
                            "paramType": "form",
                            "name": "password",
                            "type": "string",
                            "required": true,
                            "allowMultiple": false,
                            "description": "password"
                        }
                    ],
                    "responseMessages": [
                        {
                            "code": 400,
                            "message": "Invalid ID supplied"
                        },
                        {
                            "code": 404,
                            "message": "Not found"
                        }
                    ],
                    "notes": "",
                    "produces": [
                        "application/json"
                    ],
                    "authorizations": {}
                }
            ]
        }
    ]
}